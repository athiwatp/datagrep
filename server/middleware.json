{
    "initial:before": {
        "loopback#favicon": {}
    },
    "initial": {
        "compression": {},
        "cors": {
            "params": {
                "origin": true,
                "credentials": true,
                "maxAge": 86400
            }
        }
    },
    "session": {},
    "auth": {},
    "parse": {},
    "routes": {
        "loopback#rest": {
            "paths": ["${restApiRoot}"]
        }
    },
    "files": {
        "loopback#static": [{
            "params": "$!../client"
        }, {
            "paths": ["/socket.io"],
            "params": "$!../node_modules/socket.io-client"
        }, {
            "paths": ["/lib"],
            "params": "$!../node_modules"
        }, {
            "paths": ["/data"],
            "params": "$!../data"
        }]
    },
    "final": {
        "loopback#urlNotFound": {}
    },
    "final:after": {
        "loopback#errorHandler": {}
    }
}
