{
  "name": "datagrep",
  "version": "4.2.0",
  "description": "A JavaScript Artificial Intelligence & Machine Learning Library",
  "keywords": [
    "Artificial Intelligence",
    "Gradient Descent",
    "Linear Regression",
    "Logistic Regression",
    "Machine Learning",
    "Minimax",
    "pinv",
    "Pseudoinverse",
    "Singular Value Decomposition",
    "SVD"
  ],
  "main": "dist/datagrep.js",
  "module": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack",
    "clean": "rimraf dist",
    "codeclimate": "codeclimate-test-reporter < docs/coverage/lcov.info",
    "coverage": "jest --coverage",
    "coveralls": "coveralls < docs/coverage/lcov.info",
    "esdoc": "esdoc",
    "lint": "standard --verbose | snazzy",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "tdd": "jest -i --watch",
    "tdd-debug": "node --debug-brk ./node_modules/.bin/jest -i --watch",
    "test": "npm run lint && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjschock/datagrep.git"
  },
  "author": "mjschock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjschock/datagrep/issues"
  },
  "homepage": "https://github.com/mjschock/datagrep#readme",
  "dependencies": {
    "babel-runtime": "6.23.0",
    "csv-parse": "1.2.0",
    "mathjs": "3.12.3",
    "numericjs": "1.2.6",
    "vectorious": "4.8.1"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-jest": "19.0.0",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.4.0",
    "codeclimate-test-reporter": "0.4.1",
    "coveralls": "2.13.1",
    "esdoc": "0.5.2",
    "jest": "19.0.2",
    "rimraf": "2.6.1",
    "snazzy": "6.0.0",
    "standard": "10.0.2",
    "webpack": "2.4.1"
  },
  "standard": {
    "ignore": [
      "docs"
    ]
  },
  "jest": {
    "bail": true,
    "coverageDirectory": "docs/coverage",
    "coveragePathIgnorePatterns": [
      "dist",
      "docs",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "verbose": true
  }
}
