{
  "name": "datagrep",
  "version": "3.1.0",
  "description": "A JavaScript Machine Learning Library",
  "keywords": [
    "Machine Learning",
    "Gradient Descent",
    "Pseudoinverse",
    "pinv",
    "Singular Value Decomposition",
    "SVD"
  ],
  "main": "dist/datagrep.js",
  "module": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack",
    "clean": "rimraf dist",
    "codeclimate": "codeclimate-test-reporter < docs/coverage/lcov.info",
    "coverage": "jest --coverage",
    "coveralls": "coveralls < docs/coverage/lcov.info",
    "esdoc": "esdoc",
    "lint": "standard --verbose | snazzy",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "tdd": "jest -i --watch",
    "tdd-debug": "node --debug-brk ./node_modules/.bin/jest -i --watch",
    "test": "npm run lint && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjschock/datagrep.git"
  },
  "author": "mjschock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjschock/datagrep/issues"
  },
  "homepage": "https://github.com/mjschock/datagrep#readme",
  "dependencies": {
    "babel-runtime": "6.20.0",
    "csv-parse": "1.2.0",
    "numericjs": "1.2.6",
    "numjs": "0.12.4",
    "vectorious": "4.8.0"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-latest": "6.16.0",
    "codeclimate-test-reporter": "0.4.0",
    "coveralls": "2.11.15",
    "esdoc": "0.5.2",
    "jest": "18.1.0",
    "rimraf": "^2.5.4",
    "snazzy": "6.0.0",
    "standard": "8.6.0",
    "webpack": "2.2.1"
  },
  "standard": {
    "ignore": [
      "docs"
    ]
  },
  "jest": {
    "coverageDirectory": "docs/coverage",
    "coveragePathIgnorePatterns": [
      "docs",
      "/node_modules/"
    ],
    "testEnvironment": "node"
  }
}
