{
  "name": "datagrep",
  "version": "1.3.3",
  "description": "A JavaScript Machine Learning Library",
  "main": "dist/datagrep.js",
  "module": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack",
    "clean": "rimraf dist",
    "coverage": "jest --coverage",
    "coveralls": "npm run coverage && cat ./docs/coverage/lcov.info | coveralls",
    "lint": "standard --verbose | snazzy",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "tdd": "jest --watch",
    "test": "npm run lint && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjschock/datagrep.git"
  },
  "author": "mjschock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjschock/datagrep/issues"
  },
  "homepage": "https://github.com/mjschock/datagrep#readme",
  "dependencies": {
    "babel-runtime": "6.20.0",
    "csv-parse": "1.1.7",
    "numjs": "0.12.1"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-latest": "6.16.0",
    "coveralls": "2.11.15",
    "jest": "18.0.0",
    "rimraf": "^2.5.4",
    "snazzy": "5.0.0",
    "standard": "8.6.0",
    "webpack": "2.1.0-beta.28"
  },
  "standard": {
    "ignore": [
      "docs"
    ]
  },
  "jest": {
    "coverageDirectory": "docs/coverage",
    "testEnvironment": "node"
  }
}
